<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Benchmarks to be used for RL evaluation.</Description>
    <AssemblyName>Benchmarks.Threading</AssemblyName>
    <RootNamespace>Benchmarks.Threading</RootNamespace>
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
    <RestoreSources>$(RestoreSources);../../bin/nuget</RestoreSources>
    <OutputPath>..\bin\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <!--<PackageReference Include="Microsoft.PSharp" Version="1.7.0-rl" />-->
    <Reference Include="Microsoft.PSharp">
      <HintPath>..\..\bin\netcoreapp3.1\Microsoft.PSharp.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
